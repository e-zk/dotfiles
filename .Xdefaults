! ~/.Xdefaults : Xresources file

! xterm
XTerm*locale:              utf8
XTerm*loginShell:          true
XTerm*font:                -*-fixed-medium-r-*-*-12-*-*-*-*-*-*-1
XTerm*renderFont:          true
!XTerm*faceName:            monospace:size=10:style=regular
!XTerm*faceNameBold:        monospace:size=10:style=regular
XTerm*faceName:            gohufont:pixelsize=18:style=Regular
!XTerm*faceNameBold:        gohufont:pixelsize=18:style=Regular
XTerm*internalBorder:      24
XTerm*scrollBar:           0
XTerm*cursorUnderLine:     false
XTerm*disallowedWindowOps: 20,21,SetXprop

! xft
Xft.antialias: 1
Xft.autohint:  1
Xft.dpi: 96
Xft.hintstyle: hintfull

! include xresources, and colors
#include "etc/xresources"
#include "etc/colorscheme"
