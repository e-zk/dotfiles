# variables
export WHOME="/mnt/c/Users/ZZZ"
export PATH="$PATH:$HOME/bin"
export VISUAL=vis
export EDITOR=${VISUAL:-vi}

# alias
alias vim=vis
alias 230='cd "${WHOME}/_Uni/CAB230"'
alias docs='cd "${WHOME}/Documents"'

# make new react app
mkreact() {
	if [ -z $1 ]; then
		echo "usage: $0 <app-title>"
		return 1
	fi
	
	npx create-react-app $@
}
